        </activity>
        <!--<activity android:name=".FourMain.MainFragment" />
        <activity android:name=".FourMain.SchoolFragment" />
        <activity android:name=".FourMain.MajorFragment" />
        <activity android:name=".FourMain.MoreFragment" />-->
        <activity android:name=".ProfileActivity" />
        <activity android:name=".SearchActivity" />
        <activity
            android:name=".ScrollingSchoolActivity"
            android:label="@string/title_activity_scrolling_school"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ScrollingCompareActivity"
            android:label="@string/title_activity_scrolling_compare"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CompareActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>