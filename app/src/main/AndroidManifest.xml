        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">


        </activity>

        <activity android:name=".Login.LoginOneActivity" />
        <activity
            android:name=".Login.LoginTwoActivity"
            android:theme="@style/Translucent" />
        <activity android:name=".Login.LoginThreeActivity" />
        <activity
            android:name=".WelcomeActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Appwelcome">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".WelcomeGuideActivity" />
        <activity android:name=".NavigaViewActivity" />
        <activity android:name=".DrawerLayoutActivity" />
        <activity
            android:name=".CollectionActivity"
